{
  "manifest_version": 2,
  "name": "Ankush React",
  "author": "Pandit",
  "version": "1.0.1",
  "description": "chrome Extension by pandit",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "index.html"
},
  
  "permissions": ["alarms","storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}